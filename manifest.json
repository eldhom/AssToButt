{

  "description": "Changes all instances or a regex match to a specified string",
  "manifest_version": 2,
  "name": "RegexReplace",
  "version": "1.0",
  "homepage_url": "https://github.com/eldhom/RegexReplace",
  "applications": {
    "gecko": {
      "id": "regexreplace@albinvass.org",
      "strict_min_version": "45.0"
    }
  },

  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["RegexReplace.js"]
    }
  ]

}
